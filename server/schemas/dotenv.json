{
  "type": "object",
  "$id": "schema:dotenv",
  "required": [
    "MYSQL_DB",
    "MYSQL_PORT",
    "MYSQL_USER",
    "MYSQL_USER_PASS",
    "AWS_ACCESS_KEY",
    "AWS_SECRET_ACCESS_KEY",
    "AWS_LAMBDA_URL",
    "AWS_REGION"
  ],
  "properties": {
    "NODE_ENV": {
      "type": "string",
      "default": "development"
    },
    "PORT": {
      "type": "integer",
      "default": 8090
    },
    "MYSQL_HOST": {
      "type": "string",
      "default": "localhost"
    },
    "MYSQL_PORT": {
      "type": "string"
    },
    "MYSQL_DB": {
      "type": "string"
    },
    "MYSQL_USER": {
      "type": "string"
    },
    "MYSQL_USER_PASS": {
      "type": "string"
    },
    "AWS_ACCESS_KEY": {
      "type": "string"
    },
    "AWS_SECRET_ACCESS_KEY": {
      "type": "string"
    },
    "AWS_REGION": {
      "type": "string",
      "default": "eu-north-1"
    },
    "AWS_LAMBDA_URL":{
      "type":"string"
    }
  }
}
